              <String fx:value="0.25 kg" />
              <String fx:value="0.5 kg" />
              <String fx:value="1 kg" />
           
              
              </FXCollections>
              </items>
      </ChoiceBox>
      <ChoiceBox fx:id="weightChange" layoutX="414.0" layoutY="127.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="46.0">
