<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HealthTrackerController">
   <children>
      <Label text="Hi there! What is your name?" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Name:" />
            <TextField fx:id="nameTextField" />
            <Button mnemonicParsing="false" onAction="#switchToScene2" text="Enter" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Gender: " />
            <ChoiceBox fx:id="genderChoiceBox" prefWidth="150.0" />
         </children>
      </HBox>
   </children>
</VBox>
